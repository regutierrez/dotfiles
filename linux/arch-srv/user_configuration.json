    "custom_commands": [
        "usermod -aG wheel pael",
        "printf '%%wheel ALL=(ALL:ALL) ALL\\n' > /etc/sudoers.d/10-wheel",
        "chmod 440 /etc/sudoers.d/10-wheel",
        "printf \"pael ALL=(ALL) NOPASSWD: ALL\\n\" > /etc/sudoers.d/99-pael",
        "chmod 440 /etc/sudoers.d/99-pael",
        "su - pael -c \"git clone https://aur.archlinux.org/paru.git /home/pael/paru\"",
        "mkdir -p /tmp && chmod 1777 /tmp",
        "mkdir -p /var/tmp && chmod 1777 /var/tmp",
        "su - pael -c \"cd /home/pael/paru && TMPDIR=/var/tmp makepkg -si --noconfirm\"",
        "chsh -s /bin/zsh pael",
        "su - pael -c 'sh -c \"$(curl -fsLS get.chezmoi.io)\" -- init --apply regutierrez'",
        "su - pael -c \"bash -lc 'source /usr/share/nvm/init-nvm.sh && nvm install --lts && npm install -g @steipete/summarize'\"",
        "rm -f /etc/sudoers.d/99-pael",
        "printf '[Match]\\nType=ether\\n\\n[Network]\\nDHCP=yes\\n' > /etc/systemd/network/20-wired.network"
    ],
